<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#ffffbb33"
    tools:context="com.dsc.uiet.departments$PlaceholderFragment">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:id="@+id/wbe"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <WebView
                android:layout_width="match_parent" android:id="@+id/wbcse"
                android:layout_height="wrap_content"
                android:textColor="#000000" android:layout_marginLeft="5sp" android:layout_marginTop="5sp"
                android:layout_marginRight="5sp"
                android:paddingLeft="15sp"
                android:paddingTop="15sp" android:paddingRight="15sp" />
            <WebView
                android:id="@+id/wbcse1"  android:layout_below="@+id/wbcse"
                android:layout_width="match_parent" android:layout_height="wrap_content"
                android:textColor="#000000" android:layout_marginLeft="5sp" android:paddingBottom="15sp"
                android:layout_marginRight="5sp" android:layout_marginBottom="5sp"
                android:paddingLeft="15sp" android:paddingTop="5sp" android:paddingRight="15sp" />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/wbcse1"
                android:text=" "
                android:layout_marginTop="40sp"/>
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>
